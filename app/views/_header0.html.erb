<header class="header">
    <nav>
      <ul class="header-nav-right nav-links">
        <% if logged_in? %>
          <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              Account <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "Dashboard", current_user %></li>
              <li><%= link_to "Setting", edit_user_path(current_user) %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "Log out", logout_path, method: :delete %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
          <li data-turbolinks="false"><%= link_to "Sign up", new_user_path %></li>
        <% end %>
      </ul>
      <ul class="header-nav-left nav-links">
        <li><%= link_to "Home",    root_path, "data-turbolinks": "false" %></li>
        <li><%= link_to "What we do", services_path, "data-turbolinks": "false" %></li>
        <li><%= link_to "Pricing",   pricing_path %></li>
        <li><%= link_to "Help",    help_path %></li>
        <li><%= link_to "Contact us", contact_path %></li>
      </ul>
    </nav>
  <div class="header-logo">
    <%= link_to image_tag("amrr_logo.gif", alt: "App Market Response Research"), root_path, id: "logo" %>
  </div>
</header>
